{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=c:\\Projects\\job-ad-analytics\\db\\Analytics.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Warning",
      "System": "Warning",
      "TgJobAdAnalytics": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Trace",
        "TgJobAdAnalytics.Services.Uploads": "Information"
      },
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss ",
        "UseUtcTimestamp": true
      }
    }
  },
  "Rate": {
    "RateSourcePath": "..\\..\\..\\Sources"
  },
  "Upload": {
    "Mode": "Append",
    "BatchSize": 10000
  },
  "OpenAI": {
    "ProcessingChunkSize": 1000,
    "InitialConcurrency": 5,
    "MaxConcurrency": 20,
    "AdaptiveThrottleSuccessThreshold": 0.95,
    "AdaptiveThrottleWindowSize": "00:01:00",
    "AdaptiveRateLimiter": {
      "CircuitBreakerFailureThreshold": 5,
      "ConcurrencyIncrement": 1,
      "ConcurrencyDecreaseRatio": 0.3,
      "LowSuccessRateMultiplier": 0.9,
      "MinimumConcurrency": 1,
      "MinimumConcurrencyDecrement": 1,
      "MinimumSampleSize": 10
    }
  },
  "Vectorization": {
    "CurrentVersion": 1,
    "HashFunctionCount": 100,
    "LshBandCount": 20,
    "MinHashSeed": 1000,
    "NormalizationVersion": "1.1",
    "ShingleSize": 5
  },
  "SiteMetadata": {
    "BaseUrl": "https://salaries.kirilltaran.com",
    "SiteName": "Telegram Job Ad Analytics",
    "DefaultOgImagePath": null,
    "Locales": [ "en", "ru", "es" ],
    "PrimaryLocale": "ru",
    "AuthorOrg": "Tg Job Ad Analytics",
    "PublisherOrg": "Tg Job Ad Analytics",
    "JsonLdType": "Article",
    "LocalizationPath": "Locales"
  },
  "StackFiltering": {
    "TopStacksLimit": 30,
    "MinimumJobCount": 5,
    "DefaultMode": "all"
  }
}
