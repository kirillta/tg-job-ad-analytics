    "CurrentVersion": 1,
    "HashFunctionCount": 100,
    "LshBandCount": 20,
    "MinHashSeed": 1000,
    "NormalizationVersion": "1.1",
    "ShingleSize": 5
  },
  "SiteMetadata": {
    "BaseUrl": "https://salaries.kirilltaran.com",
    "SiteName": "Telegram Job Ad Analytics",
    "DefaultOgImagePath": "/assets/og/default-report.png",
    "Locales": [ "en", "ru" ],
    "PrimaryLocale": "ru",
    "AuthorOrg": "Tg Job Ad Analytics",
    "PublisherOrg": "Tg Job Ad Analytics",
    "JsonLdType": "Article",
    "LocalizationPath": "Locales"
  }
